{
  "expo": {
    "name": "lyrafy",
    "slug": "lyrafy",
    "scheme": "lyrafy",
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "platforms": [
      "ios",
      "android"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.vanshchhabra.lyrafy",
      "infoPlist": {
        "NSAppTransportSecurity": {
          "NSAllowsArbitraryLoads": true
        }
      }
    },
    "android": {
      "package": "com.vanshchhabra.lyrafy",
      "edgeToEdgeEnabled": true,
      "usesCleartextTraffic": true
    },
    "extra": {
      "eas": {
        "projectId": "764b600c-5d4f-4b0e-883f-f15cbf873cd5"
      }
    },
    "owner": "vc343",
    "plugins": [
      "expo-audio"
    ]
  }
}
