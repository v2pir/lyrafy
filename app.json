{
  "expo": {
    "name": "lyrafy",
    "slug": "lyrafy",
    "scheme": "lyrafy",
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "sdkVersion": "54.0.0",
    "platforms": [
      "ios",
      "android"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.vanshchhabra.lyrafy"
    },
    "android": {
      "package": "com.vanshchhabra.lyrafy",
      "edgeToEdgeEnabled": true
    },
    "extra": {
      "eas": {
        "projectId": "764b600c-5d4f-4b0e-883f-f15cbf873cd5"
      }
    },
    "owner": "vc343",
    "plugins": [
      "expo-audio",
      "expo-asset",
      "expo-build-properties",
      "expo-font",
      "expo-mail-composer",
      "expo-secure-store",
      "expo-sqlite",
      "expo-video",
      "expo-web-browser",
      [
        "react-native-share",
        {
          "ios": [
            "fb",
            "instagram",
            "twitter",
            "tiktoksharesdk"
          ],
          "android": [
            "com.facebook.katana",
            "com.instagram.android",
            "com.twitter.android",
            "com.zhiliaoapp.musically"
          ]
        }
      ]
    ]
  }
}
